<%= render 'shared/navbar_pages' %>
<div style="margin-top:50px;">
  <% @categories.each do |category| %>
    <div class="col-12 col-lg-12">
      <div class="container">
        <%= link_to services_path(query: category) do %>
          <div class="card categoryy">
            <img class="card-img" src="https://www.pcclean.io/wp-content/uploads/2020/4/qIZNRz.jpg" alt="Card img" >
            <div class="overlay">
              <span><%= category %></span>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  <% end %>
</div>
